{
  "name": "web-practice",
  "version": "0.0.0",
  "description": "Development practice project",
  "keywords": [
    "js",
    "frontend",
    "react"
  ],
  "config": {
    "css": {
      "options": "-u postcss-import -u postcss-url -u postcss-cssnext -u postcss-reporter"
    },
    "ghooks": {
      "pre-commit": "npm run lint"
    }
  },
  "directories": {
    "build": "build"
  },
  "presets": [
    "es2015",
    "react"
  ],
  "author": "Gergely Siladi",
  "license": "MIT",
  "engines": {
    "node": ">= 6.5.0"
  },
  "main": "src/server/index.js",
  "stylelint": {
    "extends": [
      "stylelint-config-standard"
    ]
  },
  "dependencies": {
    "express": "^4.14.0",
    "lodash": "^4.16.2",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.3",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "chai": "3.5.0",
    "css-loader": "0.25.0",
    "eslint": "3.7.0",
    "eslint-config-google": "^0.7.0",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-react": "6.3.0",
    "file-loader": "0.9.0",
    "ghooks": "^1.3.2",
    "mocha": "3.1.0",
    "postcss": "5.2.4",
    "postcss-cssnext": "2.8.0",
    "postcss-import": "8.1.2",
    "postcss-loader": "0.13.0",
    "style-loader": "0.13.1",
    "stylelint": "7.3.1",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.1"
  },
  "scripts": {
    "build": "npm run copy:html",
    "cleanup": "rm -rf build && mkdir build",
    "build:css": "postcss $npm_package_config_css_options -o $npm_package_directories_build/style.css src/client/css/index.css",
    "copy:html": "cp src/client/index.html build/",
    "dev": "npm run cleanup && npm run build && NODE_ENV=development webpack-dev-server --config webpack.config.js --hot --inline --open --host localhost",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint .",
    "lint:css": "stylelint src/client/css/index.css",
    "postinstall": "npm run build",
    "start": "npm run build && NODE_ENV=production webpack --config webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
